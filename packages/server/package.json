    "build": "tsc"
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "fastify": "^3.9.1",
    "fastify-cookie": "^4.2.0",
    "fastify-plugin": "^3.0.0",
    "fastify-session": "^5.2.1",
    "fastify-swagger": "^3.5.0",
    "got": "^11.8.1",
    "knex": "^0.21.12",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.6",
    "pino": "^6.7.0",
    "pino-pretty": "^4.3.0",
    "point-of-view": "^4.7.0",
    "socket.io": "^3.0.3"
  },
  "devDependencies": {
    "@types/pino": "^6.3.4",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "patch-package": "^6.2.2",
    "ts-node": "^9.1.1"
  }
