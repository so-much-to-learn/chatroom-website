{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run build -- --watch --outDir lib\" \"nodemon --inspect lib/index.js\" ",
